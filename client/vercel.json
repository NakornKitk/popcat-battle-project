{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/popcat-battle-project/(.*)",
        "destination": "/popcat-battle-project/index.html"
      }
    ]
    
}